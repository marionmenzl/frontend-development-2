<article>
    <h1 class="display-6">Drivers</h1>
    <table class="table">
        <thead>
            <th>Name</th>
            <th>Nationality</th>
            <th>Wikipedia</th>
        </thead>
        <tbody>
            {{#each drivers}}
                <tr>
                    <td>
                        <a href="/drivers/{{driverId}}">{{givenName}} {{familyName}}</a>
                    </td>
                    <td>
                        {{nationality}}
                    </td>
                    <td>
                        <a href="{{url}}">Goto</a>
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</article>